# TuringOS Anti-Oreo runtime profile
# Topology: Mac planner (deep ALU), Linux workers (E-core cluster)
# Assumes runtime is launched from controller host:
#   local:11434 -> Mac ollama via SSH tunnel (planner)
#   Linux worker is reached directly on tailnet IP (no SSH forwarding required)
# Tunnel helper defaults (override if needed)
TURINGOS_MAC_SSH_TARGET=zephryj@100.72.87.94
TURINGOS_MAC_SSH_IDENTITY=/home/zephryj/.ssh/id_ed25519_mac_backssh

# Core topology hard constraints
TURINGOS_HYPERCORE_V2=1
TURINGOS_STRICT_SINGLE_JSON_FRAME=1
TURINGOS_DISPATCHER_ENABLED=1
TURINGOS_DISPATCHER_MIN_HEALTH=0.45
TURINGOS_DISPATCHER_SWITCH_MARGIN=0.15
TURINGOS_HALT_VERIFY_TIMEOUT_MS=300000
TURINGOS_OLLAMA_REPAIR_ENABLED=1
TURINGOS_OLLAMA_REPAIR_MAX_ATTEMPTS=5
TURINGOS_ORACLE_REPAIR_ALL=1

# Planner lane (P-core): Mac, deep reasoning
TURINGOS_DISPATCHER_P_ORACLE=openai
TURINGOS_DISPATCHER_P_API_KEY=local
TURINGOS_DISPATCHER_P_BASE_URL=http://127.0.0.1:11434/v1
TURINGOS_DISPATCHER_P_MODEL=qwq:32b

# Worker lane (E-core): Linux, high-throughput workers
TURINGOS_DISPATCHER_E_ORACLE=openai
TURINGOS_DISPATCHER_E_API_KEY=local
TURINGOS_DISPATCHER_E_BASE_URL=http://100.64.97.113:11434/v1
TURINGOS_DISPATCHER_E_MODEL=qwen2.5:7b

# HyperCore temperatures (Anti-Oreo)
TURINGOS_HYPERCORE_PLANNER_TEMPERATURE=0.7
TURINGOS_HYPERCORE_WORKER_TEMPERATURE=0.0

# Suggested execution limits
TURINGOS_MAX_TICKS=300
TURINGOS_TIMEOUT_MS=120000
TURINGOS_MAX_OUTPUT_TOKENS=1024
